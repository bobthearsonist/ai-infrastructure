# Environment variables for Browser-Use MCP Container

# Required: API Keys (choose one or both)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: Browser Configuration
BROWSER_USE_HEADLESS=true
BROWSER_USE_LOGGING_LEVEL=WARNING
BROWSER_USE_SETUP_LOGGING=false

# Optional: Container Configuration
MCP_PORT=7009
DISPLAY=:99

# Optional: Resume and Application Settings
RESUME_PATH=/app/resume-data/resume.pdf
COVER_LETTER_TEMPLATE_PATH=/app/resume-data/cover_letter_template.txt
APPLICATION_LOG_LEVEL=INFO
