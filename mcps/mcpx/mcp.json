{
  "mcpServers": {
    "browser-use": {
      "url": "http://browser_use_proxy:7011/sse",
      "type": "sse"
    },
    "hass-mcp": {
      "url": "http://hass_mcp_proxy:7010/sse",
      "type": "sse"
    },
    "playwright": {
      "url": "http://mcp_playwright:7007/sse",
      "type": "sse"
    },
    "context7": {
      "url": "http://context7_mcp:7008/sse",
      "type": "sse"
    },
    "sequential-thinking": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "env": {},
      "icon": "#EA8685"
    },
    "memory": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-memory"],
      "env": {
        "MEMORY_FILE_PATH": "/lunar/packages/mcpx-server/config/memory.json"
      },
      "icon": "#6CC2CE"
    },
    "kapture": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "kapture-mcp", "bridge"],
      "env": {},
      "icon": "#D35400"
    }
  }
}
